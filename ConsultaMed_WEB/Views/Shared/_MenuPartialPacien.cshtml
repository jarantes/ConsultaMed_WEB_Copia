<div class="container">
    @Html.ActionLink(Roles.GetRolesForUser()[0].ToUpper(), "Index", "Home", null, new { @class = "brand" })
    <div class="nav-collapse">
        <ul class="nav">
            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Consulta<b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li>@Html.ActionLink("Agendar Consulta", "#")</li>
                    <li>@Html.ActionLink("Visualizar Consultas", "#")</li>
                </ul>
            </li>
            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Meus Dados<b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li>@Html.ActionLink("Alterar Dados", "#")</li>
                </ul>
            </li>
            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Prontuários<b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li>@Html.ActionLink("Listar Prontuários", "#")</li>
                    <li>@Html.ActionLink("Consultar Exames", "#")</li>
                    <li>@Html.ActionLink("Receitas Meédicas", "#")</li>
                </ul>
            </li>
            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Informações<b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li>@Html.ActionLink("Suporte Convênios", "#")</li>
                    <li>@Html.ActionLink("Manual da Saúde", "#")</li>
                    <li>@Html.ActionLink("Nossas Clínicas", "#")</li>
                    <li>@Html.ActionLink("Contato ConsultaMed", "#")</li>
                    <li>@Html.ActionLink("Sugestões", "#")</li>
                </ul>
            </li>
        </ul>
        <div class="navbar-search pull-right">
            <form class="fbuscar form-search">
                <div class="input-append">
                    <input class="search-query span3" id="appendedInputButton" type="text" placeholder="Buscar">
                    <button class="btn btn-inverse" type="submit"><i class="icon-white icon-search"></i></button>
                </div>
            </form>
        </div>
    </div>
</div>

